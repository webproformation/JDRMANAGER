      // Filtrer pour ne garder que les champs modifiables
      const { id, created_at, created_by, is_sample, ...editableFields } = formData;

      // Nettoyer les données: convertir les chaînes vides en null
      const cleanedData = {};
      Object.keys(editableFields).forEach(key => {
        const value = editableFields[key];
        if (value === '' || value === undefined) {
          cleanedData[key] = null;
        } else {
          cleanedData[key] = value;
        }
      });

      const dataToSave = {
        ...cleanedData
      };

      if (!item) {
