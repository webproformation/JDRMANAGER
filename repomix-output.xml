  const [activeImageCategory, setActiveImageCategory] = useState('god');
  const [activeTab, setActiveTab] = useState('info');

  if (!item) return null;

  const imageCategories = [
    { id: 'god', label: 'Le Dieu', images: item.deity_images?.god || [] },
    { id: 'temples', label: 'Temples', images: item.deity_images?.temples || [] },
    { id: 'disciples', label: 'Disciples', images: item.deity_images?.disciples || [] },
    { id: 'symbols', label: 'Symboles', images: item.deity_images?.symbols || [] }
  ];

  const domains = item.domains ? item.domains.split(',').map(d => d.trim()).filter(d => d) : [];

  const tabs = [
    { id: 'info', label: 'Informations générales', icon: Info },
    { id: 'worship', label: 'Culte & Adoration', icon: Users },
    { id: 'relations', label: 'Relations divines', icon: Network },
    { id: 'powers', label: 'Pouvoirs & Artefacts', icon: Zap },
    { id: 'gallery', label: "Galerie d'images", icon: ImageIcon },
    { id: 'notes', label: 'Notes', icon: FileText }
  ];

  const getRankIcon = (rank) => {
    switch(rank) {
      case 'overdeity': return Crown;
      case 'greater': return Star;
      case 'intermediate': return Moon;
      case 'lesser': return Sun;
      default: return Sparkles;
