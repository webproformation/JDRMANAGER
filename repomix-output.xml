<file path="src/components/EnhancedEntityDetail.jsx">
import { useState } from 'react';
import { X, Edit, Trash2, Image as ImageIcon } from 'lucide-react';

export default function EnhancedEntityDetail({
  isOpen,
  onClose,
  onEdit,
  onDelete,
  item,
  config
}) {
  const {
    entityName,
    title,
    tabs,
    imageCategories = [],
    getHeaderColor,
    getHeaderIcon
  } = config;

  const [activeTab, setActiveTab] = useState(tabs[0]?.id || 'info');
  const [activeImageCategory, setActiveImageCategory] = useState(imageCategories[0]?.id || null);

  const renderFieldValue = (field) => {
    const value = item[field.name];

    if (!value) return <span className="text-silver italic">Non renseigné</span>;

    switch (field.type) {
      case 'image':
        return value ? (
          <img src={value} alt={field.label} className="max-w-md rounded-lg shadow-lg" />
        ) : null;

      case 'images':
        return (
          <div className="space-y-4">
            {/* Category tabs */}
            <div className="flex flex-wrap gap-2">
              {field.categories.map(cat => {
                const isActive = activeImageCategory === cat.id;
                const count = value?.[cat.id]?.length || 0;
                return (
                  <button
                    key={cat.id}
                    onClick={() => setActiveImageCategory(cat.id)}
                    className={`px-4 py-2 rounded-lg text-sm font-medium transition-all ${
                      isActive
                        ? 'bg-cyan-light text-night'
                        : 'bg-arcane bg-opacity-20 text-silver hover:bg-opacity-30'
                    }`}
                  >
                    {cat.label} ({count})
                  </button>
                );
              })}
            </div>

            {/* Images grid */}
            {field.categories.map(cat => {
              if (activeImageCategory !== cat.id) return null;
              const images = value?.[cat.id] || [];
              if (images.length === 0) {
                return (
                  <p key={cat.id} className="text-silver italic">
                    Aucune image dans cette catégorie
                  </p>
                );
              }
              return (
                <div key={cat.id} className="grid grid-cols-3 gap-4">
                  {images.map((url, index) => (
                    <img
                      key={index}
                      src={url}
                      alt={`${cat.label} ${index + 1}`}
                      className="w-full h-48 object-cover rounded-lg shadow-lg hover:scale-105 transition-transform"
                    />
                  ))}
                </div>
              );
            })}
          </div>
        );

      case 'textarea':
        return (
          <p className="text-soft-white whitespace-pre-wrap leading-relaxed">
            {value}
          </p>
        );

      case 'select':
      case 'relation':
        return <span className="text-soft-white">{value}</span>;

      case 'list':
        const items = typeof value === 'string' ? value.split(',').map(i => i.trim()) : value;
        return (
          <div className="flex flex-wrap gap-2">
            {items.map((item, index) => (
              <span
                key={index}
                className="px-3 py-1 bg-cyan-light bg-opacity-20 text-cyan-light rounded-full text-sm"
              >
                {item}
              </span>
            ))}
          </div>
        );

      default:
        return <span className="text-soft-white">{value}</span>;
    }
  };

  const renderTabContent = (tab) => {
    if (!tab.fields) return null;

    return (
      <div className="space-y-6">
        {tab.fields.map(field => {
          const value = item[field.name];
          if (!value && field.hideIfEmpty) return null;

          return (
            <div key={field.name} className="space-y-2">
              <h3 className="text-cyan-light font-semibold text-lg flex items-center gap-2">
                {field.icon && <field.icon size={20} />}
                {field.label}
              </h3>
              {field.description && (
                <p className="text-silver text-sm italic">{field.description}</p>
              )}
              <div className="pl-4">
                {renderFieldValue(field)}
              </div>
            </div>
          );
        })}
      </div>
    );
  };

  if (!isOpen || !item) return null;

  const HeaderIcon = getHeaderIcon ? getHeaderIcon(item) : null;
  const headerColor = getHeaderColor ? getHeaderColor(item) : 'from-cyan-light/20 to-arcane/40';

  return (
    <div className="fixed inset-0 bg-black bg-opacity-70 backdrop-blur-sm flex items-center justify-center z-50 p-4 overflow-y-auto">
      <div className="bg-night bg-opacity-95 backdrop-blur-md rounded-lg border border-arcane border-opacity-50 shadow-2xl shadow-arcane max-w-6xl w-full my-8">
        {/* Hero Header */}
        <div className={`relative h-64 bg-gradient-to-br ${headerColor} overflow-hidden rounded-t-lg`}>
          {item.image && (
            <img
              src={item.image}
              alt={item.name}
              className="absolute inset-0 w-full h-full object-cover opacity-30"
            />
          )}

          <div className="absolute inset-0 bg-gradient-to-t from-night via-night/50 to-transparent" />

          {/* Action buttons */}
          <div className="absolute top-4 right-4 flex gap-2">
            <button
              onClick={onEdit}
              className="p-2 bg-cyan-light bg-opacity-20 backdrop-blur-md rounded-full hover:bg-opacity-30 transition-all text-cyan-light"
            >
              <Edit size={20} />
            </button>
            <button
              onClick={onDelete}
              className="p-2 bg-red-500 bg-opacity-20 backdrop-blur-md rounded-full hover:bg-opacity-30 transition-all text-red-400"
            >
              <Trash2 size={20} />
            </button>
            <button
              onClick={onClose}
              className="p-2 bg-arcane bg-opacity-30 backdrop-blur-md rounded-full hover:bg-opacity-50 transition-all text-silver"
            >
              <X size={20} />
            </button>
          </div>

          {/* Entity info */}
          <div className="absolute bottom-6 left-8 flex items-end gap-6">
            {/* Icon or portrait */}
            {HeaderIcon && (
              <div className="w-24 h-24 rounded-full bg-cyan-light bg-opacity-20 backdrop-blur-md border-4 border-cyan-light flex items-center justify-center shadow-lg">
                <HeaderIcon size={48} className="text-cyan-light" />
              </div>
            )}

            {/* Name and subtitle */}
            <div>
              <h1 className="text-4xl font-bold text-soft-white drop-shadow-lg mb-2">
                {item.name}
              </h1>
              {item.subtitle && (
                <p className="text-xl text-cyan-light italic font-light drop-shadow-md">
                  {item.subtitle}
                </p>
              )}
            </div>
          </div>
        </div>

        {/* Tabs Navigation */}
        <div className="sticky top-0 bg-gradient-to-b from-night to-night/95 backdrop-blur-xl border-b border-cyan-light/20 shadow-lg z-10">
          <div className="px-4 py-3">
            <div className="flex flex-wrap gap-2 justify-center">
              {tabs.map((tab) => {
                const Icon = tab.icon;
                const isActive = activeTab === tab.id;
                return (
                  <button
                    key={tab.id}
                    onClick={() => setActiveTab(tab.id)}
                    className={`group relative flex items-center gap-2 px-4 py-2 rounded-xl text-sm font-semibold transition-all duration-300 ${
                      isActive
                        ? 'bg-cyan-light text-night shadow-lg shadow-cyan-light/50 scale-105'
                        : 'bg-arcane/20 text-silver hover:bg-arcane/40 hover:text-cyan-light hover:scale-105 border border-arcane/30'
                    }`}
                  >
                    <Icon size={18} className={isActive ? 'animate-pulse' : 'group-hover:rotate-12 transition-transform'} />
                    <span>{tab.label}</span>
                    {isActive && (
                      <div className="absolute -bottom-1 left-1/2 transform -translate-x-1/2 w-1/2 h-1 bg-cyan-light rounded-full shadow-lg shadow-cyan-light/50"></div>
                    )}
                  </button>
                );
              })}
            </div>
          </div>
        </div>

        {/* Content */}
        <div className="p-8">
          {tabs.map(tab => (
            activeTab === tab.id && (
              <div key={tab.id}>
                {renderTabContent(tab)}
              </div>
            )
          ))}
        </div>

        {/* Footer metadata */}
        <div className="border-t border-arcane border-opacity-30 p-4 bg-arcane bg-opacity-10">
          <div className="flex justify-between text-sm text-silver">
            {item.created_at && (
              <span>Créé le {new Date(item.created_at).toLocaleDateString()}</span>
            )}
            {item.updated_at && (
              <span>Modifié le {new Date(item.updated_at).toLocaleDateString()}</span>
            )}
          </div>
        </div>
      </div>
    </div>
  );
}
