import { useState, useEffect } from 'react';
import { supabase } from '../lib/supabase';
import { ChevronDown, Loader } from 'lucide-react';

export default function RelationSelect({
  tableName,
  value,
  onChange,
  label,
  required = false,
  filterBy = null,
  filterValue = null,
  placeholder = "Sélectionner..."
}) {
  const [options, setOptions] = useState([]);
  const [loading, setLoading] = useState(true);

  useEffect(() => {
    loadOptions();
  }, [tableName, filterBy, filterValue]);

  const loadOptions = async () => {
    setLoading(true);
    try {
      let query = supabase
        .from(tableName)
        .select('id, name')
        .order('name');

      if (filterBy && filterValue) {
        query = query.eq(filterBy, filterValue);
      }

      const { data, error } = await query;

      if (error) throw error;
      setOptions(data || []);
    } catch (error) {
      console.error(`Erreur lors du chargement de ${tableName}:`, error);
      setOptions([]);
    } finally {
      setLoading(false);
    }
  };

  return (
    <div>
      <label className="block text-sm font-semibold text-soft-white mb-2">
        {label}
        {required && <span className="text-red-400 ml-1">*</span>}
      </label>

      <div className="relative">
        <select
          value={value || ''}
          onChange={(e) => onChange(e.target.value)}
          required={required}
          disabled={loading}
          className="w-full px-4 py-2 bg-night bg-opacity-60 border border-arcane border-opacity-40 text-soft-white rounded-lg focus:ring-2 focus:ring-cyan-light focus:border-cyan-light transition-all appearance-none pr-10 disabled:opacity-50 disabled:cursor-not-allowed"
        >
          <option value="" className="bg-night">
            {loading ? 'Chargement...' : placeholder}
          </option>
          {options.map(option => (
            <option key={option.id} value={option.id} className="bg-night">
              {option.name}
            </option>
          ))}
        </select>

        <div className="absolute right-3 top-1/2 -translate-y-1/2 pointer-events-none text-silver">
          {loading ? (
            <Loader size={16} className="animate-spin" />
          ) : (
            <ChevronDown size={16} />
          )}
        </div>
      </div>

      {!loading && options.length === 0 && (
        <p className="text-sm text-amber-400 mt-1">
          Aucune option disponible. Créez d'abord des entrées dans la table {tableName}.
        </p>
      )}
    </div>
  );
}
</file>

