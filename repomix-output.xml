      }

      console.log('[DeityForm] Données à sauvegarder (nettoyées):', dataToSave);
      console.log('[DeityForm] Type de deity_images:', typeof dataToSave.deity_images);

      let savedId;

      if (item) {
        console.log('[DeityForm] Mise à jour de la divinité ID:', item.id);

        // Utiliser l'Edge Function pour contourner les problèmes RLS
        const apiUrl = `${import.meta.env.VITE_SUPABASE_URL}/functions/v1/update-entity`;

        const response = await fetch(apiUrl, {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json',
            'Authorization': `Bearer ${import.meta.env.VITE_SUPABASE_ANON_KEY}`
          },
          body: JSON.stringify({
            tableName: 'deities',
            id: item.id,
            data: dataToSave
          })
        });

        const result = await response.json();

        if (!response.ok || result.error) {
          console.error('[DeityForm] Erreur UPDATE via Edge Function:', result.error);
          throw new Error(result.error || 'Erreur lors de la mise à jour');
        }

        console.log('[DeityForm] UPDATE réussi via Edge Function:', result.data);
        savedId = item.id;
      } else {
        console.log('[DeityForm] Création d\'une nouvelle divinité');
        const { data: insertData, error: insertError } = await supabase
          .from('deities')
          .insert([dataToSave])
          .select('id')
          .maybeSingle();

        if (insertError) {
          console.error('[DeityForm] Erreur INSERT:', insertError);
          throw insertError;
        }
        savedId = insertData.id;
      }

      console.log('[DeityForm] ID sauvegardé:', savedId);

      const { data: fetchedData, error: fetchError } = await supabase
        .from('deities')
        .select('*')
        .eq('id', savedId)
        .maybeSingle();

      console.log('[DeityForm] Données récupérées:', fetchedData);

      if (fetchError) {
        console.error('[DeityForm] Erreur lors de la récupération:', fetchError);
        throw fetchError;
      }

      if (!fetchedData) {
        throw new Error('Impossible de récupérer les données après la sauvegarde');
      }

      onSave(fetchedData);
      onClose();
    } catch (error) {
      console.error('[DeityForm] Erreur lors de la sauvegarde:', error);
      console.error('[DeityForm] Stack:', error.stack);
      alert('Erreur lors de la sauvegarde: ' + error.message);
