async function importData() {
  console.log('🚀 Démarrage de l\'importation des données...\n');

  const dataDir = path.join(__dirname, '../src/data');
  const files = fs.readdirSync(dataDir).filter(f => f.endsWith('.csv') && !f.includes('copy'));

  const orderedFiles = ['world.csv', 'continent.csv', 'country.csv', 'location.csv'];
  const otherFiles = files.filter(f => !orderedFiles.includes(f));
  const processFiles = [...orderedFiles.filter(f => files.includes(f)), ...otherFiles];

  for (const file of processFiles) {
    const csvName = file.replace('.csv', '');
    const tableName = tableNames[csvName];
    const mapping = mappings[csvName];

    if (!tableName || !mapping) {
      console.log(`⚠️  Ignoré: ${file} (pas de mapping défini)`);
      continue;
    }

    try {
      console.log(`📄 Lecture de ${file}...`);
      const buffer = fs.readFileSync(path.join(dataDir, file));

      const detectedEncoding = chardet.detect(buffer);
      console.log(`   Encodage détecté: ${detectedEncoding}`);

      let content;
      if (detectedEncoding && detectedEncoding.toLowerCase() !== 'utf-8' && detectedEncoding.toLowerCase() !== 'ascii') {
        content = iconv.decode(buffer, detectedEncoding);
        console.log(`   Conversion ${detectedEncoding} → UTF-8`);
      } else {
        content = buffer.toString('utf-8');
      }

      const csvData = parseCsv(content);

      if (csvData.length === 0) {
        console.log(`   ℹ️  Aucune donnée dans ${file}\n`);
        continue;
      }

      console.log(`   Trouvé ${csvData.length} entrées`);

      const supabaseData = csvData.map(row => mapToSupabase(row, mapping));

      const { data, error } = await supabase
        .from(tableName)
        .insert(supabaseData)
        .select();

      if (error) {
        console.error(`   ❌ Erreur lors de l'insertion dans ${tableName}:`, error.message);
      } else {
        console.log(`   ✅ ${data.length} entrées importées dans ${tableName}\n`);
      }

    } catch (error) {
      console.error(`   ❌ Erreur lors du traitement de ${file}:`, error.message, '\n');
    }
  }

  console.log('✨ Importation terminée!');
}

importData().catch(console.error);
