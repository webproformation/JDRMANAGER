import { useState, useEffect } from 'react';
import { X, Save, Sparkles, Image as ImageIcon, Shield } from 'lucide-react';
import { supabase } from '../lib/supabase';
import ImageUpload from './ImageUpload';
import StaticSelect from './StaticSelect';
import RelationSelect from './RelationSelect';

export default function EnhancedEntityForm({
  isOpen,
  onClose,
  onSuccess,
  item = null,
  config
}) {
  const {
    entityName,
    tableName,
    title,
    tabs,
    imageCategories = [],
    gmImageCategories = [],
    hasGMNotes = false
  } = config;

  const [activeTab, setActiveTab] = useState(tabs[0]?.id || 'general');
  const [activeImageCategory, setActiveImageCategory] = useState(imageCategories[0]?.id || null);
  const [activeGMImageCategory, setActiveGMImageCategory] = useState(gmImageCategories[0]?.id || null);
  const [loading, setLoading] = useState(false);
  const [error, setError] = useState('');
  const [formData, setFormData] = useState({});

  useEffect(() => {
    if (item) {
      setFormData(item);
    } else {
      const initialData = {};
      tabs.forEach(tab => {
        tab.fields?.forEach(field => {
          if (field.type === 'images') {
            initialData[field.name] = item?.[field.name] || {};
          } else {
            initialData[field.name] = item?.[field.name] || '';
          }
        });
      });
      setFormData(initialData);
    }
  }, [item, tabs]);

  const handleChange = (name, value) => {
    setFormData(prev => ({ ...prev, [name]: value }));
  };

  const handleImageCategoryUpload = (fieldName, category, url) => {
    setFormData(prev => ({
      ...prev,
      [fieldName]: {
        ...prev[fieldName],
        [category]: [...(prev[fieldName]?.[category] || []), url]
      }
    }));
  };

  const handleImageCategoryDelete = (fieldName, category, index) => {
    setFormData(prev => ({
      ...prev,
      [fieldName]: {
        ...prev[fieldName],
        [category]: prev[fieldName]?.[category]?.filter((_, i) => i !== index) || []
      }
    }));
  };

  const handleSubmit = async (e) => {
    e.preventDefault();
    setLoading(true);
    setError('');

    try {
      const dataToSave = { ...formData };

      if (item?.id) {
        const { error: updateError } = await supabase
          .from(tableName)
          .update(dataToSave)
          .eq('id', item.id);

        if (updateError) throw updateError;
      } else {
        const { error: insertError } = await supabase
          .from(tableName)
          .insert([dataToSave]);

        if (insertError) throw insertError;
      }

      onSuccess();
      onClose();
    } catch (err) {
      setError(err.message);
    } finally {
      setLoading(false);
    }
  };

  const renderField = (field) => {
    const value = formData[field.name] || '';

    switch (field.type) {
      case 'text':
        return (
          <input
            type="text"
            value={value}
            onChange={(e) => handleChange(field.name, e.target.value)}
            className="w-full px-4 py-2 bg-arcane bg-opacity-30 border border-cyan-light border-opacity-30 rounded-lg text-soft-white focus:outline-none focus:border-cyan-light"
            placeholder={field.placeholder}
            required={field.required}
          />
        );

      case 'number':
        return (
          <input
            type="number"
            value={value}
            onChange={(e) => handleChange(field.name, e.target.value)}
            className="w-full px-4 py-2 bg-arcane bg-opacity-30 border border-cyan-light border-opacity-30 rounded-lg text-soft-white focus:outline-none focus:border-cyan-light"
            placeholder={field.placeholder}
            required={field.required}
          />
        );

      case 'textarea':
        return (
          <textarea
            value={value}
            onChange={(e) => handleChange(field.name, e.target.value)}
            rows={field.rows || 4}
            className="w-full px-4 py-2 bg-arcane bg-opacity-30 border border-cyan-light border-opacity-30 rounded-lg text-soft-white focus:outline-none focus:border-cyan-light resize-none"
            placeholder={field.placeholder}
            required={field.required}
          />
        );

      case 'select':
        return (
          <StaticSelect
            options={field.options}
            value={value}
            onChange={(val) => handleChange(field.name, val)}
            placeholder={field.placeholder}
          />
        );

      case 'relation':
        return (
          <RelationSelect
            table={field.table}
            value={value}
            onChange={(val) => handleChange(field.name, val)}
            placeholder={field.placeholder}
            filterBy={field.filterBy}
            filterValue={formData[field.filterValue]}
          />
        );

      case 'image':
        return (
          <ImageUpload
            bucket={field.bucket || 'images'}
            onChange={(url) => handleChange(field.name, url)}
            value={value}
            label={field.label}
          />
        );

      case 'images':
        return (
          <div className="space-y-4">
            {/* Category tabs */}
            <div className="flex flex-wrap gap-2 border-b border-cyan-light border-opacity-20 pb-2">
              {field.categories.map(cat => {
                const isActive = activeImageCategory === cat.id;
                const count = formData[field.name]?.[cat.id]?.length || 0;
                return (
                  <button
                    key={cat.id}
                    type="button"
                    onClick={() => setActiveImageCategory(cat.id)}
                    className={`px-4 py-2 rounded-lg text-sm font-medium transition-all ${
                      isActive
                        ? 'bg-cyan-light text-night'
                        : 'bg-arcane bg-opacity-20 text-silver hover:bg-opacity-30'
                    }`}
                  >
                    {cat.label} ({count})
                  </button>
                );
              })}
            </div>

            {/* Image upload for active category */}
            {field.categories.map(cat => {
              if (activeImageCategory !== cat.id) return null;
              return (
                <div key={cat.id} className="space-y-4">
                  <ImageUpload
                    bucket={field.bucket || 'images'}
                    onChange={(url) => handleImageCategoryUpload(field.name, cat.id, url)}
                    value=""
                    label={cat.label}
                  />

                  {/* Display uploaded images */}
                  {formData[field.name]?.[cat.id]?.length > 0 && (
                    <div className="grid grid-cols-3 gap-4">
                      {formData[field.name][cat.id].map((url, index) => (
                        <div key={index} className="relative group">
                          <img
                            src={url}
                            alt={`${cat.label} ${index + 1}`}
                            className="w-full h-32 object-cover rounded-lg"
                          />
                          <button
                            type="button"
                            onClick={() => handleImageCategoryDelete(field.name, cat.id, index)}
                            className="absolute top-2 right-2 p-1 bg-red-500 rounded-full opacity-0 group-hover:opacity-100 transition-opacity"
                          >
                            <X size={16} />
                          </button>
                        </div>
                      ))}
                    </div>
                  )}
                </div>
              );
            })}
          </div>
        );

      default:
        return null;
    }
  };

  const renderTabContent = (tab) => {
    if (!tab.fields) return null;

    return (
      <div className="space-y-6">
        {tab.fields.map(field => (
          <div key={field.name} className="space-y-2">
            <label className="block text-cyan-light font-medium">
              {field.label}
              {field.required && <span className="text-red-400 ml-1">*</span>}
            </label>
            {field.description && (
              <p className="text-silver text-sm">{field.description}</p>
            )}
            {renderField(field)}
          </div>
        ))}
      </div>
    );
  };

  if (!isOpen) return null;

  return (
    <div className="fixed inset-0 bg-black bg-opacity-70 backdrop-blur-sm flex items-center justify-center z-50 p-4">
      <div className="bg-night bg-opacity-95 backdrop-blur-md rounded-lg border border-arcane border-opacity-50 shadow-2xl shadow-arcane max-w-6xl w-full max-h-[90vh] overflow-y-auto">
        {/* Header */}
        <div className="sticky top-0 bg-night bg-opacity-90 backdrop-blur-md border-b border-arcane border-opacity-30 p-6 flex justify-between items-center z-20">
          <h2 className="text-3xl font-bold text-cyan-light">
            {item ? `Modifier ${entityName}` : `Créer ${entityName}`}
          </h2>
          <button
            onClick={onClose}
            className="text-silver hover:text-soft-white transition-colors"
          >
            <X size={24} />
          </button>
        </div>

        {/* Tabs */}
        <div className="sticky top-[88px] bg-night bg-opacity-90 backdrop-blur-md border-b border-arcane border-opacity-30 z-10">
          <div className="flex flex-wrap justify-center gap-1">
            {tabs.map(tab => {
              const Icon = tab.icon;
              const isActive = activeTab === tab.id;
              return (
                <button
                  key={tab.id}
                  type="button"
                  onClick={() => setActiveTab(tab.id)}
                  className={`flex items-center gap-1.5 px-3 py-2.5 text-sm font-medium transition-all whitespace-nowrap ${
                    isActive
                      ? 'text-cyan-light border-b-2 border-cyan-light bg-arcane bg-opacity-20'
                      : 'text-silver hover:text-soft-white hover:bg-arcane hover:bg-opacity-10'
                  }`}
                >
                  <Icon size={16} />
                  {tab.label}
                </button>
              );
            })}
          </div>
        </div>

        {/* Form */}
        <form onSubmit={handleSubmit} className="p-6">
          {error && (
            <div className="mb-6 p-4 bg-red-500 bg-opacity-20 border border-red-500 rounded-lg text-red-400">
              {error}
            </div>
          )}

          {/* Tab content */}
          {tabs.map(tab => (
            activeTab === tab.id && (
              <div key={tab.id}>
                {renderTabContent(tab)}
              </div>
            )
          ))}

          {/* Submit button */}
          <div className="mt-8 flex justify-end gap-4 sticky bottom-0 bg-night bg-opacity-95 backdrop-blur-md py-4 border-t border-arcane border-opacity-30">
            <button
              type="button"
              onClick={onClose}
              className="px-6 py-2 bg-arcane bg-opacity-30 text-silver rounded-lg hover:bg-opacity-50 transition-all"
            >
              Annuler
            </button>
            <button
              type="submit"
              disabled={loading}
              className="px-6 py-2 bg-cyan-light text-night rounded-lg hover:bg-opacity-90 transition-all flex items-center gap-2 disabled:opacity-50"
            >
              <Save size={20} />
              {loading ? 'Sauvegarde...' : 'Sauvegarder'}
            </button>
          </div>
        </form>
      </div>
    </div>
  );
}
